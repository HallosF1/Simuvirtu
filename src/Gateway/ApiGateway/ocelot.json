{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5246
        }
      ],
      "UpstreamPathTemplate": "/app/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "DownstreamHeaderTransform": {
        "Authorization": "Bearer {Token}"
      }
    },
    {
      "DownstreamPathTemplate": "/analysis/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8000
        }
      ],
      "UpstreamPathTemplate": "/analysis/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "DownstreamHeaderTransform": {
        "Authorization": "Bearer {Token}"
      }
    },
    {
      "DownstreamPathTemplate": "/signals/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8000
        }
      ],
      "UpstreamPathTemplate": "/signals/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "DownstreamHeaderTransform": {
        "Authorization": "Bearer {Token}"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7000",
    "RequestIdKey": "Oc-Request-Id"
  }
}
